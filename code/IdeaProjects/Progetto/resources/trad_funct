#include <stdio.h>
#include <stdlib.h>

int a = 2;
int b = 3;

void funct1() {
    int d = 3;

    if (d == 2) {
       a = 3;
    } else {
        a = 4;
    }
}

void funct2() {
    int e = 3;

    while (e < 5) {
        e += 2;
    }
}

int main() {
    int c = 4;
    a = 5;

    funct1();

    return 0;
}